{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 2,
    "prettier/prettier": "error",
    "quotes": [2, "double", { "avoidEscape": true }]
  }
}
